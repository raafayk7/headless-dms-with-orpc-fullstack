{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "ct-starter",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "prettier": "catalog:",
    "turbo": "^2.5.2",
    "typescript": "catalog:",
    "taze": "catalog:"
  },
  "engines": {
    "node": ">=22"
  },
  "workspaces": {
    "packages": ["packages/*", "apps/*"],
    "catalog": {
      "typescript": "^5.8.3",
      "@types/node": "^22.15.17",
      "@biomejs/biome": "2.0.0-beta.6",
      "prettier": "^3.5.3",
      "@orpc/contract": "^1.2.0",
      "@orpc/zod": "^1.2.0",
      "zod": "^3.25.0",
      "taze": "^19.1.0",
      "better-auth": "^1.2.7"
    }
  }
}
